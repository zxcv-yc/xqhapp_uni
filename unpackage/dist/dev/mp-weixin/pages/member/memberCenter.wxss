
.top_bg_box {
	width: 100%;
	height: 310rpx;
	background-size: 100% 100%;
}
.text-FFCA95 {
	color: #FFCA95;
}
.title_img {
	width: 100rpx;
}
.tq_item {
	width: calc(100% / 3);
}
.tq_item_img {
	width: 100rpx;
	margin: 0 auto;
}
.tq_box {
	padding: 60rpx;
}
.gift_item {
	border-radius: 10rpx;
	margin-bottom: 30rpx;
}
.gift_item_btn {
	padding: 5rpx 15rpx;
	border-radius: 999px;
	color: #FFCA95;
	background-color: #2C2C2C;
}
.gift_item_img image {
	width: 150rpx;
	height: 150rpx;
	border-radius: 10rpx;
}
.gift_item_info {
	width: calc(100% - 170rpx);
	height: 150rpx;
}



page {
	background-color: transparent !important;
}
.container {
	background-color: transparent !important;
}
.content {
	/* padding: 20upx; */
	padding: 30rpx 0;
	position: relative;
	z-index: 2;
}
#bannerSwiper {
	height: 280rpx;
	min-height: 200rpx !important;
	border-radius: 40rpx;
	overflow: hidden;
}
.search_box {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	margin-left: 80rpx;
}
.swiper-item {
	border-radius: 40rpx;
	overflow: hidden;
}
.grid_menu_item_box image {
	display: block;
	margin: 0 auto;
	width: 80%;
	height: 80rpx;
}
.screen_item_active {
	color: #ff6f9c;
}
.qvan_block {
	width: 32rpx;
	height: 32rpx;
	text-align: center;
	font-size: 20rpx;
	line-height: 32rpx;
	margin-right: 10rpx;
	background-color: #f94e74;
	color: #fff;
	border-radius: 4rpx;
}
.grid_menu_item_box {
	width: calc(20% - 30rpx);
	text-align: center;
	margin: 15rpx;
}
.banner_swiper_box {
	margin: 40rpx 0;
	position: relative;
	z-index: 2;
	/* padding: 0 30upx; */
	box-shadow: 0 0.3em 0.8em rgba(255, 111, 156, 0.3);
}
.grid_menu_item_text {
	margin-top: 30rpx;
	font-size: 24rpx;
}
#menuSwiper {
	margin-bottom: 60rpx;
	height: 400rpx;
}
.margin-right-xl {
	margin-right: 40rpx;
}
.storeList {
	background: -webkit-linear-gradient(top, #fff 0%, #F5EFEF 150px, #F5EFEF 100%);
	background: linear-gradient(to bottom, #fff 0%, #F5EFEF 150px, #F5EFEF 100%);
	padding: 30rpx;
	padding-top: 0;
}
.storeList_item_img_box {
	width: 184rpx;
	height: 184rpx;
	border-radius: 10rpx;
	position: relative;
}
.storeList_item_img_tag {
	width: 115rpx;
	height: 40rpx;
	text-align: center;

	background-size: 100% 100%;
	border-radius: 0 10rpx 0 25rpx;
	position: absolute;
	top: 0;
	right: 0;
	z-index: 2;
	color: #fff;
	font-size: 24rpx;
}
.storeList_item_img {
	width: 184rpx;
	height: 184rpx;
	border-radius: 10rpx;
}
.storeList_item {
	background-color: #fff;
	padding: 30rpx 20rpx;
	border-radius: 10rpx;
	margin: 20rpx 0;
}
.storeList_item_info_box {
	width: calc(100% - 214rpx);
}
.storeList_item_tag {
	margin: 10rpx 20rpx 10rpx 0;
}
.screen_box {
	width: 100%;
	/* padding: 24upx; */
	background-color: #fff;
	padding: 24rpx 30rpx;
}
.al-items-start {
	-webkit-box-align: start !important;
	-webkit-align-items: flex-start !important;
	        align-items: flex-start !important;
}
.sort_type_box {
	width: 100%;
	position: absolute;
	top: 80rpx;
	background-color: #fff;
	/* z-index: 1071; */
	padding: 0 30rpx;
}
.sort_type_item {
	padding: 20rpx 0;
	border-bottom: solid #f5f5f5 1rpx;
}
.screen_big_box {
	position: relative;
	z-index: 2000;
}
._mask {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background-color: rgba(0, 0, 0, .5);
	z-index: 1000;
}

